{
  "name": "jsonforms-tooling-theia",
  "displayName": "JSONForms Tooling",
  "description": "Extension for the VS Code editor to support JSONForms Tooling, making it easier for developers to start new project or try out the library.",
  "version": "0.0.1",
  "publisher": "EclipseSource",
  "license": "MIT",
  "keywords": [
    "theia-plugin"
  ],
  "files": [
    "src"
  ],
  "dependencies": {
    "jsonforms-tooling-common": "file:../jsonforms-tooling-common"
  },
  "devDependencies": {
    "@theia/plugin": "0.4.0",
    "@theia/plugin-packager": "0.0.1-1550255725",
    "@types/node": "^11.10.4",
    "rimraf": "2.6.3",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "clean": "rimraf out",
    "watch": "tsc -watch",
    "compile": "tsc -p ./",
    "build": "yarn run compile",
    "postinstall": " ",
    "custominstall": "yarn install && yarn run fixlink",
    "fixlink": "npm link ../jsonforms-tooling-common",
    "lint": "tslint --project tsconfig.json"
  },
  "engines": {
    "theiaPlugin": "latest"
  },
  "theiaPlugin": {
    "backend": "out/extension.js"
  }
}
